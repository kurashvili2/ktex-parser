{
  "name": "@kurashvili2/ktex-parser",
  "module": "src/index.ts",
  "type": "module",
  "dependencies": {
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "prettier": {
    "printWidth": 100,
    "importOrder": [
      "^@core/(.*)$",
      "^[.~/]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}